[application]

config/name="Sapphire"
config/description="So yeah, this was made my second week with Godot for the 2-minute horror jam. It's decent asf imo"
run/main_scene="res://Scenes/UI/SplashScreen.tscn"
boot_splash/image="res://Assets/Deployment/HeavenlyBlueLogo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://Assets/Deployment/sapphirelogoMain.png"
config/macos_native_icon="res://Assets/Deployment/sapphirelogoMac.icns"
config/windows_native_icon="res://Assets/Deployment/SapphireLogoWin.ico"

[autoload]

GlobalPlayer="*res://Scripts/Global/GLOBAL_PLAYER.gd"
GlobalUi="*res://Scripts/Global/GLOBAL_UI.gd"

[display]

window/size/width=960
window/size/height=409
window/size/resizable=false
window/size/test_width=960
window/size/test_height=540
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

